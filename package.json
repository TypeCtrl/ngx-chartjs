{
  "name": "@ctrl/ngx-chartjs",
  "repository": "scttcper/ngx-chartjs",
  "version": "0.0.0-placeholder",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "test": "ng test ---watch=false --browsers=ChromeCI",
    "test:watch": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "link": "cd dist && npm link",
    "lint": "ng lint ngx-chartjs --type-check --format stylish",
    "lint:fix": "ng lint ngx-chartjs --type-check --fix --format stylish",
    "ghpages": "del-cli dist && ng build --prod --no-progress"
  },
  "private": true,
  "dependencies": {
    "chart.js": "3.1.0"
  },
  "devDependencies": {
    "@angular/common": "11.2.9",
    "@angular/compiler": "11.2.9",
    "@angular/core": "11.2.9",
    "@angular/forms": "11.2.9",
    "@angular/platform-browser": "11.2.9",
    "@angular/platform-browser-dynamic": "11.2.9",
    "@angular/router": "11.2.9",
    "@ctrl/tinycolor": "3.4.0",
    "core-js": "3.10.1",
    "patternomaly": "1.3.2",
    "rxjs": "6.6.7",
    "tslib": "2.2.0",
    "zone.js": "0.11.4",
    "@angular-devkit/build-angular": "0.1102.8",
    "@angular/cli": "11.2.8",
    "@angular/compiler-cli": "11.2.9",
    "@angular/language-service": "11.2.9",
    "@types/jasmine": "3.6.9",
    "@types/node": "14.14.37",
    "codelyzer": "6.0.1",
    "del-cli": "3.0.1",
    "jasmine-core": "3.7.1",
    "karma": "6.3.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "11.2.4",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  },
  "engines": {
    "npm": ">= 6",
    "node": ">= 12"
  },
  "release": {
    "branch": "master"
  }
