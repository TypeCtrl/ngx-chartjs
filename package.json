{
  "name": "@ctrl/ngx-chartjs",
  "repository": "scttcper/ngx-chartjs",
  "version": "0.0.0-placeholder",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "test": "ng test ---watch=false --browsers=ChromeCI",
    "test:watch": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "link": "cd dist && npm link",
    "lint": "ng lint ngx-chartjs --type-check --format stylish",
    "lint:fix": "ng lint ngx-chartjs --type-check --fix --format stylish",
    "ghpages": "del-cli dist && ng build --prod --no-progress"
  },
  "private": true,
  "dependencies": {
    "chart.js": "2.9.3",
    "@types/chart.js": "2.9.24"
  },
  "devDependencies": {
    "@angular/common": "10.1.2",
    "@angular/compiler": "10.1.2",
    "@angular/core": "10.1.2",
    "@angular/forms": "10.1.2",
    "@angular/platform-browser": "10.1.2",
    "@angular/platform-browser-dynamic": "10.1.2",
    "@angular/router": "10.1.2",
    "@ctrl/tinycolor": "3.1.4",
    "core-js": "3.6.5",
    "patternomaly": "1.3.2",
    "rxjs": "6.6.3",
    "tslib": "2.0.1",
    "zone.js": "0.11.1",
    "@angular-devkit/build-angular": "0.1001.2",
    "@angular/cli": "10.1.2",
    "@angular/compiler-cli": "10.1.2",
    "@angular/language-service": "10.1.2",
    "@types/jasmine": "3.5.14",
    "@types/node": "14.11.1",
    "codelyzer": "6.0.0",
    "del-cli": "3.0.1",
    "jasmine-core": "3.6.0",
    "karma": "5.2.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "10.1.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  },
  "engines": {
    "npm": ">= 6",
    "node": ">= 12"
  },
  "release": {
    "branch": "master"
  }
}
